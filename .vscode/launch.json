{
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mymold",
            "args": [
              "-o",
              "main",
              "a.o",
              "b.o"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test_demo/demo",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
              {
                "description": "为 gdb 启用整齐打印",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ]
        },
    ]
}